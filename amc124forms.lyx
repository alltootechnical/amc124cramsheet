\begin_layout Subsection
Partitions
\end_layout

\begin_layout Standard
The function 
\begin_inset Formula $p\left(n,k\right)$
\end_inset

 denotes the number of ways of writing 
\begin_inset Formula $n$
\end_inset

 as a sum of exactly 
\begin_inset Formula $k$
\end_inset

 terms.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p\left(n,k\right)=\begin{cases}
1 & \text{if }n=k=0\\
0 & \text{if }n<k\\
p\left(n-1,k-1\right)+p\left(n-k,k\right) & \text{if }n\ge k
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Subsection
Stirling Numbers
\end_layout

\begin_layout Subsubsection*
First Kind (Cycles)
\end_layout

\begin_layout Standard
Counts number of permutations of 
\begin_inset Formula $n$
\end_inset

 elements with 
\begin_inset Formula $k$
\end_inset

 disjoint cycles.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left[{n\atop k}\right]=\begin{cases}
1 & \text{if }n=k=0\\
0 & \text{if }n\ne k\wedge k=0\\
\left(n-1\right)\left[{n-1\atop k}\right]+\left[{n-1\atop k-1}\right] & \text{if }n,k>0
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Second Kind (Subsets)
\end_layout

\begin_layout Standard
Counts the number of ways to partition a set of 
\begin_inset Formula $n$
\end_inset

 objects into 
\begin_inset Formula $k$
\end_inset

 non-empty subsets.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\{ {n\atop k}\right\} =\begin{cases}
1 & \text{if }n=k=0\\
0 & \text{if }n\ne k\wedge k=0\\
\left(k-1\right)\left\{ {n-1\atop k}\right\} +\left\{ {n-1\atop k-1}\right\}  & \text{if }n,k>0
\end{cases}
\]

\end_inset


\end_layout

